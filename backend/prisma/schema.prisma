datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Session {
  id        String   @id
  sid       String   @unique
  data      String
  expiresAt DateTime
}

model User {
  id          Int     @default(autoincrement()) @id
  login       String  @unique
  email       String? @unique
  name        String?
  avatar      String?
  intraId     Int     @unique
  banner      String?
  fortytwoId  String?  @unique
}